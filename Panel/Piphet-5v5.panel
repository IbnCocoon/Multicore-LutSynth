      <luaMethod luaMethodName="packDsiData" luaMethodCode="function packDsiData(unpackedData)&#10;&#13;&#10;    local packedData = MemoryBlock(1171)&#13;&#10;    local count = unpackedData:getSize() - 1&#13;&#10;    local paramPos = 0&#13;&#10;    local packingBytePos = 0&#13;&#10;  &#13;&#10;    for i=0,count,1 do&#13;&#10;&#13;&#10;        local val = unpackedData:getByte(i)&#13;&#10;&#13;&#10;        if paramPos == 0 then&#13;&#10;             packingBytePos = packedData:getSize()&#13;&#10;             packedData:append(MemoryBlock{0})&#13;&#10;        end&#13;&#10;&#13;&#10;        if bit.band(val, 128) == 128 then&#13;&#10;            val = val - 128&#13;&#10;            local packingByte = packedData:getByte(packingBytePos)&#13;&#10;            packedData:setByte(packingBytePos, bit.bor(packingByte, bit.lshift(1, paramPos)))&#13;&#10;        end&#13;&#10;&#13;&#10;        packedData:append(MemoryBlock{val})&#13;&#10;        paramPos = math.mod(paramPos + 1, 7)&#13;&#10;    end&#13;&#10;&#13;&#10;    return packedData &#10;end"
                 luaMethodLinkedProperty="" luaMethodSource="0" uuid="36d4550b060244d49093c899dfb7833a"
                 luaMethodValid="1"/>
      <luaMethod luaMethodName="unpackDsiData" luaMethodCode="function unpackDsiData(packedData)&#10;&#13;&#10;    local unpackedData = MemoryBlock(1024)&#13;&#10;    local paramPos = 0;&#13;&#10;    local packingByte = 0&#13;&#10;    local count = packedData:getSize() - 1&#13;&#10;    local size = 0&#13;&#10;&#13;&#10;    for i=0,count,1 do&#13;&#10;&#13;&#10;        local val = packedData:getByte(i)&#13;&#10;&#13;&#10;        if paramPos == 0 then&#13;&#10;            packingByte = val&#13;&#10;        else&#13;&#10;            if bit.band(packingByte, bit.lshift(1, paramPos - 1)) ~= 0 then&#13;&#10;                val = bit.bor(val, 128)&#13;&#10;            end&#13;&#10;            unpackedData:insert(MemoryBlock{val}, size)&#13;&#10;            size = size + 1&#13;&#10;        end&#13;&#10;        paramPos = math.mod(paramPos + 1, 8)&#13;&#10;    end&#13;&#10;&#13;&#10;    return unpackedData&#10;end"
                 luaMethodLinkedProperty="" luaMethodSource="0" uuid="fefe0c9ca62b43e7b805fb0cb473d7b9"
                 luaMethodValid="1"/>
