      <luaMethod luaMethodName="saveToFile" luaMethodCode="--&#10;-- Called when a modulator value changes&#10;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#10;-- @value    new numeric value of the modulator&#10;--&#10;saveToFile = function(--[[ CtrlrModulator --]] mod, --[[ number --]] value, --[[ number --]] source)&#13;&#10;&#13;&#10;    if panel:getBootstrapState() == true then&#13;&#10;        return&#13;&#10;    end&#13;&#10;&#13;&#10;&#9;console(&quot;Saving to file...&quot;)&#13;&#10;&#13;&#10;    local programData = packDsiData(createProgramDataMessage())&#13;&#10;    local programEditCommand = &quot;f0 01 2d 03&quot;..&quot; &quot;..programData:toHexString(1)..&quot; &quot;..&quot;f7&quot;&#13;&#10;&#9;local data = MemoryBlock(programEditCommand)&#13;&#10;&#9;&#13;&#10;&#9;local file = utils.saveFileWindow(&quot;Save program&quot;, File(&quot;&quot;), &quot;*.syx&quot;, true)&#13;&#10;&#13;&#10;&#9;if file:isValid() == false then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;if file:existsAsFile() == false then&#13;&#10;&#9;&#9;if file:create() == false then&#13;&#10;&#9;&#9;&#9;utils.warnWindow(&quot;Save file&quot;, &quot;The file does not exist.&quot;)&#13;&#10;&#9;&#9;&#9;return&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;if file:replaceWithData(data) == false then&#13;&#10;&#9;&#9;utils.warnWindow(&quot;Save file&quot;, &quot;Failed to write data to file.&quot;)&#13;&#10;        return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;console(&quot;Saved to file&quot;)&#10;end"
